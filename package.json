{
  "name": "muzi_backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:local": "NODE_ENV=local nest start --watch",
    "start:debug": "NODE_ENV=local nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:local": "pnpm docker:up && dotenv -e environments/.env.local -- jest --passWithNoTests --runInBand",
    "test:ci": "pnpm docker:up && pnpm prisma:migrate:test && dotenv -e environments/.env.test -- jest --passWithNoTests --ci --reporters=default --reporters=jest-junit --runInBand && pnpm docker:down",
    "test:cov:local": "jest --coverage",
    "prisma:generate": "npx prisma generate",
    "prisma:push:local": "dotenv -e environments/.env.local -- pnpm exec prisma db push",
    "prisma:migrate:local": "dotenv -e environments/.env.local -- pnpm exec prisma migrate dev",
    "prisma:migrate:test": "dotenv -e environments/.env.test -- pnpm exec prisma migrate dev",
    "prisma:deploy:prod": "dotenv -e environments/.env.prod -- npx prisma migrate deploy",
    "prepare": "husky || true",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.3.1",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.3.1",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.2.0",
    "@prisma/client": "5.8.1",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "cheerio": "1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "luxon": "^3.4.4",
    "puppeteer": "^21.11.0",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.10",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "axios-mock-adapter": "^1.22.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.6",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.2.4",
    "prisma": "^5.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.{!(module|*interface|*type|*dto|*controller),}.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
